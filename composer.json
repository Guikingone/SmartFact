{
    "name": "guikingone/smartfact",
    "license": "proprietary",
    "type": "project",
    "description": "The source code of the smartfact project.",
    "require": {
        "php": "^7.1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "^1.2",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^2.7",
        "doctrine/doctrine-fixtures-bundle": "^2.2@dev",
        "lexik/jwt-authentication-bundle": "^2.4",
        "nelmio/cors-bundle": "^1.5",
        "overblog/graphql-bundle": "^0.10.0",
        "phpunit/phpunit": "^6.2",
        "predis/predis": "^1.1",
        "snc/redis-bundle": "^2.0@dev",
        "symfony/asset": "^3.3",
        "symfony/browser-kit": "^3.3",
        "symfony/cache": "^3.3",
        "symfony/dom-crawler": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/phpunit-bridge": "^3.3",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "^3.3",
        "symfony/security-guard": "^3.3",
        "symfony/serializer": "^3.3",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/translation": "^3.3",
        "symfony/twig-bundle": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/workflow": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "symfony/dotenv": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BP249CRD9FS685G5M3QDCNW2",
            "allow-contrib": "true"
        }
    }
}
