{% extends 'base.html.twig' %}

{% block title %}SmartFact - Login{% endblock %}

{% block body %}
    {% if errors %}
        <div>
            {{ errors.messageKey|trans(error.messageData, 'security') }}
        </div>
    {% endif %}

    {{ form_start(form) }}
        <button type="submit">
            Submit
        </button>
    {{ form_end(form) }}
{% endblock %}
